{
  "name": "@swirl/cli",
  "description": "JSONRPC utility",
  "version": "0.0.0",
  "author": "Nicholas J. Paterno @nicholasjpaterno",
  "bin": {
    "swirl": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/trufflesuite/swirl/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-plugins": "^1.7.9",
    "@swirl/core": "0.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/mkdirp": "^1.0.0",
    "dotenv": "^8.2.0",
    "env-paths": "^2.2.0",
    "inquirer": "^7.1.0",
    "mkdirp": "^1.0.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/trufflesuite/swirl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "swirl",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "eth": {
        "description": "Ethereum JSONRPC Methods"
      }
    }
  },
  "repository": "trufflesuite/swirl",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "yarn run rmDist && yarn run compile",
    "rmDist": "rm -rf ./dist",
    "clean": "rm -rf ./node_modules ./dist",
    "compile": "tsc -p tsconfig.json"
  },
  "types": "lib/index.d.ts"
}
